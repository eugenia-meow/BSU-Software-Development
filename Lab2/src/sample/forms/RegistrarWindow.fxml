<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RegistrarWindowController">
    <children>
        <Button fx:id="buttonSwitchRegistration" layoutX="14.0" layoutY="260.0" mnemonicParsing="false" onAction="#onSwitchRegistration" text="Switch Registration" />
      <Button layoutX="316.0" layoutY="260.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="52.0" text="Exit" />
      <BorderPane layoutX="14.0" layoutY="50.0" prefHeight="200.0" prefWidth="174.0" style="-fx-border-color: #aaa;">
         <center>
            <TitledPane collapsible="false" prefHeight="200.0" prefWidth="168.0" text="Professors" BorderPane.alignment="CENTER">
               <content>
                  <ListView prefHeight="200.0" prefWidth="200.0" />
               </content>
            </TitledPane>
         </center>
         <bottom>
            <ToolBar prefHeight="32.0" prefWidth="173.0" BorderPane.alignment="CENTER">
               <items>
                  <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="52.0" text="Add">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Button>
                  <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="52.0" text="Edit" />
                  <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="52.0" text="Delete" />
               </items>
               <padding>
                  <Insets bottom="2.0" left="4.0" right="2.0" top="2.0" />
               </padding>
            </ToolBar>
         </bottom>
      </BorderPane>
      <Label fx:id="labelName" layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="353.0" text="Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
