<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SubmitGradesController">
   <children>
      <ComboBox fx:id="comboBox" layoutX="91.0" layoutY="14.0" onAction="#onSelect" prefHeight="26.0" prefWidth="296.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <TableView fx:id="tableView" editable="true" layoutX="14.0" layoutY="50.0" prefHeight="200.0" prefWidth="374.0">
        <columns>
          <TableColumn editable="false" prefWidth="299.20001220703125" text="Student" />
          <TableColumn prefWidth="52.800048828125" text="Grade" />
        </columns>
      </TableView>
      <Button cancelButton="true" layoutX="337.0" layoutY="261.0" mnemonicParsing="false" onAction="#onCancel" text="Cancel">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button defaultButton="true" layoutX="278.0" layoutY="261.0" mnemonicParsing="false" onAction="#onApply" prefHeight="26.0" prefWidth="51.0" text="Apply">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="78.0" text="Select Course" textAlignment="RIGHT" />
   </children>
</AnchorPane>
